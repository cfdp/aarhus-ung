            <SectionDetails id="1226" parentID="1219" level="6" writerID="1" creatorID="1" nodeType="1087" template="1086" sortOrder="8" createDate="2013-02-22T13:50:42" updateDate="2013-06-03T08:26:00" nodeName="Skriv ansøgning og CV" urlName="skriv-ansoegning-og-cv" writerName="aarhusung" creatorName="aarhusung" path="-1,1069,1076,1092,1134,1219,1226" isDoc="">
              <sectionText>
              </sectionText>
              <appSectionText>
              </appSectionText>
