            <SectionDetails id="1158" parentID="1143" level="6" writerID="1" creatorID="1" nodeType="1087" template="1086" sortOrder="8" createDate="2013-02-21T10:18:16" updateDate="2013-06-03T08:26:09" nodeName="Aarhus Katedralskole" urlName="aarhus-katedralskole" writerName="aarhusung" creatorName="aarhusung" path="-1,1069,1076,1092,1142,1143,1158" isDoc="">
