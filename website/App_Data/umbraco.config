                    <imageCrop>
                      <crops date="2013-03-13T12:07:06">
                        <crop name="FrontpageSlide" x="0" y="11" x2="365" y2="175" url="/media/24729/AarhusTech_FrontpageSlide.jpg" />
                        <crop name="SectionSlide" x="0" y="11" x2="365" y2="175" url="/media/24729/AarhusTech_SectionSlide.jpg" />
                        <crop name="SectionIcon" x="103" y="17" x2="260" y2="120" url="/media/24729/AarhusTech_SectionIcon.jpg" />
                        <crop name="AboutImage" x="89" y="0" x2="276" y2="187" url="/media/24729/AarhusTech_AboutImage.jpg" />
                      </crops>
                    </imageCrop>
