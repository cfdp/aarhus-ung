            <sectionText>
            </sectionText>
            <appSectionText>Aarhus Tech tilbyder mange forskellige uddannelser</appSectionText>
            <logo>
              <DAMP fullMedia="">
                <mediaItem>
                  <Image id="1824" version="043bcf7d-9d28-49a0-9d2e-828021f0dd92" parentID="1381" level="4" writerID="1" nodeType="1032" template="0" sortOrder="0" createDate="2013-03-13T12:07:06" updateDate="2013-03-13T12:27:42" nodeName="Aarhustech" urlName="aarhustech" writerName="aarhusung" nodeTypeAlias="Image" path="-1,1303,1377,1381,1824">
                    <umbracoFile>/media/24729/AarhusTech.PNG</umbracoFile>
                    <umbracoWidth>365</umbracoWidth>
                    <umbracoHeight>187</umbracoHeight>
                    <umbracoBytes>18497</umbracoBytes>
