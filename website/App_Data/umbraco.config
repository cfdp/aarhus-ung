            <SectionDetails id="1151" parentID="1143" level="6" writerID="1" creatorID="1" nodeType="1087" template="1086" sortOrder="1" createDate="2013-02-21T10:00:22" updateDate="2013-06-03T08:26:03" nodeName="Egå Gymnasium" urlName="egaa-gymnasium" writerName="aarhusung" creatorName="aarhusung" path="-1,1069,1076,1092,1142,1143,1151" isDoc="">
